<?xml version="1.0"?>
<errorList moduleName="misc">
   <error id="1">
      <errName>NULL_PARAM</errName>
      <errSeverity>SEVERE</errSeverity>
      <errFormat><![CDATA[received a NULL value for parameter '%s'.]]></errFormat>
   </error>
   <error id="2">
      <errName>MEM_FAILURE</errName>
      <errSeverity>SEVERE</errSeverity>
      <errFormat><![CDATA[could not allocate or copy memory.]]></errFormat>
   </error>
   <error id="3">
      <errName>FUNC_CALL</errName>
      <errSeverity>SEVERE</errSeverity>
      <errFormat><![CDATA[could not perform '%s' function call.]]></errFormat>
   </error>
   <error id="4">
      <errName>DYN_BUF_BAD_POSITION</errName>
      <errSeverity>SEVERE</errSeverity>
      <errFormat><![CDATA['%s' parameter outside of the Dynamic Buffer range.]]></errFormat>
   </error>
   <error id="5">
      <errName>DYN_BUF_BAD_FROM_TO</errName>
      <errSeverity>SEVERE</errSeverity>
      <errFormat><![CDATA[received bad 'from' and 'to' parameters ('to' lower than 'from').]]></errFormat>
   </error>
   <error id="6">
      <errName>FILE_ENV_VAR_NOT_DEF</errName>
      <errSeverity>WARNING</errSeverity>
      <errFormat><![CDATA['%s' environment variable is not defined.]]></errFormat>
   </error>
</errorList>
